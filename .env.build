# Arquivo .env.build - Variáveis para build Docker
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# URL temporária para build (não conecta de verdade)
DATABASE_URL="postgresql://dummy:dummy@localhost:5432/dummy"

# Secrets temporários para build
NEXTAUTH_SECRET="temporary-build-secret-not-used-in-production"
NEXTAUTH_URL="http://localhost:3000"

# Email config (dummy para build)
EMAIL_PASSWORD="dummy"
EMAIL="dummy@email.com"

# Flag para indicar que está em build
SKIP_DB_CONNECTION=true